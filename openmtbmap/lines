## Copy of overlays in order not to make mistakes....
# 0x303: 0x13, 0x10609
# 0x304: 0x13, 0x1040b
# 0x306: 0x13, 0x1040d
# 0x309: 0x13, 0x10106
# 0x30d: 0x13, 0x1061a
# 0x30e: 0x13, 0x10618
# 0x310: 0x13, 0x10119

## 0x1001b superfluos?

# Not shown on Oregon apart from noted in typfile:
# 0x1000a to 0x100ff
# Not shown POI regions
# 0x119??


# Remove dirty spelling error:
higwhay=* & highway!=* {set highway='${higwhay}'}

# Remove crap leftover from imports or correct crap found via tagwatch.stoecker.eu
ref=0 {delete ref}
highway=footpath {set highway=path}
tracktype=grade1_ruts {set tracktype=2}


highway=service_footway {set highway=footway}
highway="track; footway" {set highway=footway}
highway="track;footway" {set highway=footway}
highway="path;track" {set highway=path}
highway=Feldweg {set highway=track}
highway="path; track" {set highway=path}
highway="track; path" {set highway=track}
highway="track;path" {set highway=track}
highway="footway; track" {set highway=footway}
highway="footway;track" {set highway=footway}
highway="residential; track" {set highway=track}
highway="residential;track" {set highway=track}
highway="tertiary;track" {set highway=track}
highway=residental {set highway=residential}
highway=livingstreet {set highway=living_street}
highway="footway; cycleway" {set highway=cycleway; set segregated=no}
highway="footway;cycleway" {set highway=cycleway; set segregated=no}
highway="cycleway; footway" {set highway=cycleway; set segregated=no}
highway="cycleway;footway" {set highway=cycleway; set segregated=no}
highway=cycleroad {set highway=cycleway}
highway=cyclestreet {set highway=cycleway}
highway=pedestrian & ( sac_scale=* | mtb:scale=* ) {set highway=path}
hiking=true {set hiking=yes}
mtb=true {set mtb=yes}
( highway=footway | highway=pedestrian ) & ( hiking=yes | mtb=yes ) {set highway=path}
( highway=service | highway=footway | highway=bridleway ) & sport=cycling {set highway=cycleway}
highway=minor {set highway=unclassified}
(oneway=reverse | oneway=opposite ) {set oneway=-1}
(oneway=true | oneway=yes | oneway=forward | oneway=1) {set oneway=yes}
oneway=* & oneway!=-1 & oneway!=yes {delete oneway}
route=mountainbike {set route=mtb}
( highway=via_ferrata | highway=via_feratta | highway=via_ferratta ) { set highway=path; set sport=via_feratta)
highway=trail {set highway=path}
highway=rest_area {set highway=residential}
highway="road;track" {set highway=track}
highway="service;track" {set highway=track}
highway="track;unclassified" {set highway=track}
highway=unused {set highway=road}
highway=yes {set highway=road}

highway=residential & access=destination & foot=designated & bicycle=designated {set highway=cycleway}
highway=residential & foot=designated & bicycle=designated {set highway=path}
highway=residential & bicycle=official {set highway=path}

( cycleway=no | cycleway=none ) {delete cycleway}
cycleway="lane;opposite_lane" {set cycleway=opposite_lane}
cycleway="lane/opposite_lane" {set cycleway=opposite_lane}
cycleway="track;opposite_track" {set cycleway=opposite_track}
cycleway="opposite_track;track" {set cycleway=opposite_track}
bicycle=opposite_lane {add cycleway=opposite_lane}
cycleway=opposite_share_busway {set cycleway=opposite_lane}
bicycle=opposite_share_busway {add cycleway=opposite_lane}
bicycle=opposite_track {add cycleway=opposite_track}
bicycle=lane {add cycleway=lane}
bicycle=track {add cycleway=track}

service=drive-through | service=drive_through {add class:bicycle=-2}

( cycleway:left=path | cycleway=left | cycleway:left=yes ) {add cycleway:left=track}
( cycleway:right=path | cycleway=right | cycleway:right=yes ) {add cycleway:right=track}
( cycleway=segregated | cycleway=path ) {add cycleway=track}


bridge=footbridge {set highway=path; set bridge=yes}
bridge=pedestrian {set highway=pedestrian; set bridge=yes}
bridge=bicycle {set highway=cycleway; set bridge=yes}

highway=raceway {set aeroway=runway}

access=true {set access=yes}
highway=* & access=yes {set acccesss=yes; delete access}

# Remove access rights for motorcars/motorcycles:
motorcar=no | motorcar=private {delete motorcar}
motorcycle=no | motorcycle=private {delete motorcycle}
motor_vehicle=* {delete motor_vehicle}
hgv=* {delete hgv}
psv=* {delete psv}
taxi=* {delete taxi}
bus=* {delete bus}
emergency=no | emergency=private {delete emergency}
delivery=no | delivery=private {delete delivery}
goods=no | goods=private {delete goods}
carpool=* {delete carpool}


# Make sure that no route=bicycle or route=mtb is missing because of another route
extremecarver=mr & route!=mtb  & route!=bicycle {set route=mtb}
extremecarver5=bike & route!=bicycle & route!=mtb & extremecarver!=mr {set route=bicycle}

# Get special bicycle keys
bicycle:surface=* {set surface = '${bicycle:surface}'}
bicycle:tracktype=* {set tracktype = '${bicycle:tracktype}'}
bicycle:smoothness=* {set smoothness = '${bicycle:smoothness}'}

cycleway:surface=* {set surface = '${cycleway:surface}'}
cycleway:tracktype=* {set tracktype = '${cycleway:tracktype}'}
cycleway:smoothness=* {set smoothness = '${cycleway:smoothness}'}


# Create keys for higher resolution
# highway=secondary & ref ~ '[L].*' {set importantsecondary=yes}
# highway=secondary & ref ~ '[B].*' {set importantsecondary=yes}
highway=secondary & int_ref=* {set importantsecondary=yes}
highway=primary & int_ref=* {set importantprimary=yes}
highway=tertiary & int_ref=* {set importanttertiary=yes}
highway=tertiary & ref ~ '[L].*' {set importanttertiary=yes}

# Remove planned roads/railways
( highway=proposed | highway=planned ) {delete highway; delete oneway; delete tunnel; delete bridge}
( railway=proposed | railway=planned ) {delete railway; delete oneway; delete tunnel; delete bridge}
planned=true & ( highway=* | railway=* ) {set planned=yes}
planned=yes & highway=* {delete highway; delete oneway; delete tunnel; delete bridge}
planned=yes & railway=* {delete railway; delete oneway; delete tunnel; delete bridge}
( highway=proposed | highway=planned ) & tunnel=* {delete tunnel}
( highway=proposed | highway=planned ) & bridge=* {delete bridge}
( highway=proposed | highway=planned ) & oneway=* {delete oneway}
( highway=proposed | highway=planned ) {delete highway}
( railway=proposed | railway=planned ) & tunnel=* {delete tunnel}
( railway=proposed | railway=planned ) & bridge=* {delete bridge}
( railway=proposed | railway=planned ) & oneway=* {delete oneway}
( railway=proposed | railway=planned | railway=dismantled ) {delete railway}


bicycle=no & highway!=* & route!=* & aerialway!=* {delete bicycle}
construction=no | construction=minor  {delete construction}
construction=* & highway=* & route!=bicycle & route!=mtb & extremecarver5!=bike  {set highway=construction}
highway=construction {add construction=yes}

# Remove oneway on Waterways
oneway=* & waterway=* {delete oneway}

# Remap toll=yes or toll=true
(toll=yes | toll=true | (toll=* & toll!=no & toll!=false & toll!=snowmobile & toll!=unknown)) {delete toll; set toll1=yes}

# Correct Spelling Mistakes and Apply Translitset name=
bicycle=no 	{set bicycle=nope}
highway=footpath {set highway=footway}
tracktype=1_ruts {set tracktype=2}
route=bicycle & network=mtb {set route=mtb}

# Translate unusual access values
bicycle=agricultural {add bicycle=yes}
bicycle=forestry {add bicycle=yes}
bicycle=delivery {add bicycle=yes}
bicycle=permissive {set bicycle=yes}
bicycle=private {add access=destination; set mkgmap:unpaved=1 }

foot=agricultural {set foot=yes}
foot=forestry {set foot=yes}
foot=delivery & (bicycle!=* | bicycle=no | bicycle=nope | bicycle=delivery) & cycleway!=* {set access=destination; set mkgmap:unpaved=1; set toll=yes}
foot=permissive {set foot=yes}
foot=private & (bicycle!=* | bicycle=no | bicycle=nope) & cycleway!=* {set access=no; set foot=no; set mkgmap:unpaved=1; set toll=yes }


access=destination & bicycle!=* {set access=yes}
access=agricultural | access=forestry {set access=yes}
vehicle=agricultural | vehicle=forestry {set access=yes}

(impassable=yes | disused=yes ) & highway=* {set taxi=no; set mkgmap:unpaved=1}

# Placeholder for Translitset name=s.
loc_name=* 								{set name='${loc_name}'}
int_name=* 								{set name='${int_name}'}
int_name!=* & name:en=* 				{set name='${name:en}'}
int_name!=* & name:en!=* & name:de=* 	{set name='${name:de}'}
name:mtb=*								{set name='${name:mtb}'}
mtb:name=*								{set name='${mtb:name}'}
name_mtb=*								{set name='${name_mtb}'}
mtb_name=*								{set name='${mtb_name}'}



######## Create far zoomed out highways first so that hopefully the join-lines code works out better due to less splits by different names.

highway=motorway & int_ref=*	{set name='${ref} ${name}'} 	[0x10013 resolution 15-15 continue]
highway=motorway 				{set name='${ref} ${name}'} 	[0x10013 resolution 16-20 continue]
# highway=motorway_junction 									[0x10f10 resolution 19-19 continue]
highway=motorway_link | highway=motorway_junction 				[0x10f10 resolution 20-20 continue]

highway=trunk & int_ref=* 	{set name='${ref} ${name}'} 		[0x10f0c resolution 16-17 continue]
highway=trunk 				{set name='${ref} ${name}'} 		[0x10f0c resolution 18-20 continue]
highway=trunk_link 												[0x10f13 resolution 20-20 continue]

highway=primary & importantprimary=yes {set name='${ref} ${name}'} 					[0x10f18 resolution 18-18 continue]

(motorroad=true | motoroad=true) & highway=* {set motorroad=yes}
motoroad=yes {set motorroad=yes}
motorroad=yes {set access=no; set motorcar=no; set bicycle=no; set foot=no; set hgv=no; set taxi=no; set emergency=no; set mkgmap:unpaved=1; set toll=yes}
(bicycle=nope | bicycle=no ) & highway!=motorway & highway!=motorway_link  {set name='${name} XBK'|'XBK'; set mkgmap:unpaved=1; add taxi=no}

highway=primary & oneway=* & lanes>1 {set no0x150e=yes}
highway=primary & oneway=* & lanes>1 {set name='${ref} ${name}'}					[0x10f18 resolution 19-20 continue]
highway=primary & no0x150e!=yes	{set name='${ref} ${name}'} 						[0x10f0d resolution 19-20 continue]
highway=primary_link 																[0x10f17 resolution 20-20 continue]


highway=secondary & ( importantsecondary=yes | importantprimary=yes ) {set name='${ref} ${name}'} [0x10f19 resolution 19-19 continue]
highway=secondary & oneway=yes & lanes>1 {set no0x150e=yes}
highway=secondary & oneway=yes & lanes>1 {set name='${ref} ${name}'}				[0x10f19 resolution 20-20 continue]
highway=secondary & no0x150e!=yes	{set name='${ref} ${name}'}						[0x10f1e resolution 20-20 continue]
highway=secondary_link 																[0x10f19 resolution 20-20 continue]

highway=tertiary 					{set name='${ref} ${name}'}						[0x10f0f resolution 20-20 continue]



# Naming Section
railwayname=* & railway=* & highway!=* {set name='${railwayname}${name}' | '${railwayname}'}

# Add Route Name without setting it
route_name_mtb1=* 		{add route_name1='${route_name_mtb1}'}
route_name_mtb2=* 		{add route_name1='${route_name_mtb2}'}
route_name_bike1=* 		{add route_name1='${route_name_bike1}'}
route_name_mtb3=* 		{add route_name1='${route_name_mtb3}'}
route_name_bike2=* 		{add route_name1='${route_name_bike2}'}
route_name_bike3=* 		{add route_name1='${route_name_bike3}'}
route_name_hiking=* 	{add route_name1='${route_name_hiking}'}
route_name_walking=*	{add route_name1='${route_name_walking}'}
route_name_walking=*	{add route_name1='${route_name_foot}'}

1ximportance=* {add ximportance='${1ximportance}'}
2ximportance=* {add ximportance='${2ximportance}'}
3ximportance=* {add ximportance='${3ximportance}'}
4ximportance=* {add ximportance='${4ximportance}'}


# Classify mtb Routes based on objective criteria:

# ascent/descent are usally more important than plain distance
# route=mtb & ascent>10000 {add ximportance=icn}
# route=mtb & ascent>4500 {add ximportance=ncn}
# route=mtb & ascent>1399 {add ximportance=rcn}
# route=mtb & ascent<1400 & ( descent!=* | descent<2000 )  {add ximportnace=lcn}
# route=mtb & ascent<1400 {add ximportance=rcn}
# route=mtb & descent>5000 {add xiportance=rcn}
# descent>5000 but few ascent is usually a shuttle assisted freeride tour

# route=mtb & distance>300 {add ximportance=icn}
# route=mtb & distance>120 {add ximportance=ncn}
# route=mtb & distance>51  {add ximportance=rcn}
# route=mtb & distance<50  {add ximportance=lcn}

# route=mtb & ( ximportance=icn | ximportance=ncn )  & roundtrip=yes {set ximportance=rcn ; delete roundtrip}


# Classifiy cycle routes based on distance

1xnetwork=icn {set extremecarver6=icn}
2xnetwork=ncn {add extremecarver6=ncn}
3xnetwork=rcn {add extremecarver6=rcn}
4xnetwork=lcn {add extremecarver6=lcn}

#route=bicycle & distance>800 {set network=icn}
#route=bicycle & distance>300 {set network=ncn}
#route=bicycle & distance>50 {set network=rcn}


# Remove explicit sidewalks, that are not explicitely allows for bicycles.
highway=* & sidewalk=true {set sidewalk=yes}
highway=footway & ( footway=sidewalk | sidewalk=yes) & (bicycle!=* | bicycle=no | bicycle=nope) & (cycleway!=* | cycleway=no) & mtblayer!=* & extremecarver6!=* & route!=bicycle & route!=mtb & route!=hiking & sac_scale!=* & !(tracktype>1) & mtb:scale!=* {delete highway}

# Hide unaccessible tunnels
tunnel=true {set tunnel=yes}
highway=* & tunnel=yes & (access=private|access=no) & foot!=yes & bicycle!=yes & route!=mtb & route!=hiking & route!=bicycle {delete highway; delete junction; delete tunnel; delete oneway}
# Disable dead-end-checks for unaccessible oneways
highway=* & oneway=yes & (access=private|access=no) {add mkgmap:dead-end-check=false}


# Overlay Attributes

